@model Fashion.ERP.Web.Areas.Almoxarifado.Models.DepositoMaterialModel

<div class="block-flat">
    @using (Html.BeginForm(MVC.Almoxarifado.DepositoMaterial.Novo(), FormMethod.Post, new { id="form", @class = "form-horizontal" })) {
	    @Html.AntiForgeryToken()
        @Html.ValidationSummaryEx()
        @Html.Partial("_NovoOuEditar", Model)
        <div class="form-group">
            <div class="col-sm-offset-1 col-sm-10 topOffset-15">
                <button id="btnSubmit" class="btn btn-primary" type="submit" data-loading-text="Aguarde...">Salvar</button>
            </div>
        </div>
    }
</div>
